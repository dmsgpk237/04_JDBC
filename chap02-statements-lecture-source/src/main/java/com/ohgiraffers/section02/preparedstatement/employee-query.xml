<?xml version="1.0" encoding="utf-8" ?> <!-- 선언부 : 항상 제일 첫 번째에 들어가야 함.-->
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <comment/>
<!-- 키 -->
        <entry key="selectEmpByFamilyName">
<!-- 벨류 -->
            select
            emp_no,
            emp_id,
            emp_name,
            dept_code,
            salary,
            job_code
            from
            employee
            where
            emp_name like concat(?, '%')
        </entry>

</properties>